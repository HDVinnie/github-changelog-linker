{
    "name": "hdvinnie/github-changelog-linker",
    "description": "Generates beautiful CHANGELOG.md with links to PRs, versions and users grouped in Added/Changed/Fixed/Removed categories.",
    "type": "package",
    "keywords": [
        "github",
        "changelog",
        "HDVinnie"
    ],
    "homepage": "https://github.com/HDVinnie/github-changelog-linker",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker"
    ],
    "authors": [
        {
            "name": "HDVinnie",
            "email": "hdinnovations@protonmail.com",
            "homepage": "https://github.com/HDVinnie",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "nette/utils": "^3.2",
        "symfony/console": "^4.4|^5.4|^6.0",
        "symfony/http-kernel": "^4.4|^5.4|^6.0",
        "symfony/process": "^4.4|^5.4|^6.0",
        "symplify/package-builder": "^9.3",
        "symplify/smart-file-system": "^9.3",
        "symplify/set-config-resolver": "^9.3",
        "symplify/symplify-kernel": "^9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
